{    
    "name": "AutoGPT BETA",
    "version": "1.0.1",
    "description": "una explicacion de mi extension",
    "manifest_version": 3,
    "permissions": ["activeTab","downloads"],

    "host_permissions": ["https://*/*", "http://*/*"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/1.png",
            "32": "images/2.png",
            "48": "images/3.png",
            "128": "images/4.png"
          }

      },
    
      "icons":{
        "16": "images/1.png",
        "32": "images/2.png",
        "48": "images/3.png",
        "128": "images/4.png"
      }, "web_accessible_resources": [
        {
          "resources": ["gpt.js"],
          "matches": ["<all_urls>"]
        }
    ],

      "content_scripts": [
        {
          "matches": ["https://chat.openai.com/*"],

          "js": [
            "./content/interfaz.js",
            "./content/componentes.js",
            "./content/gestorDeDatos.js",
            "./content/gpt.js",
            "./content/content-script.js"
          ]
        }
      ]

  }
